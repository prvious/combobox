{
    "name": "prvious/filament-combobox",
    "description": "Combobox for Filament Panel",
    "keywords": [
        "prvious",
        "laravel",
        "filament-combobox"
    ],
    "homepage": "https://github.com/prvious/filament-combobox",
    "support": {
        "issues": "https://github.com/prvious/filament-combobox/issues",
        "source": "https://github.com/prvious/filament-combobox"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Clovis Muneza",
            "email": "mugaro234@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/forms": "^4.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.25",
        "nunomaduro/collision": "^8.8.2",
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Prvious\\Filament\\Combobox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Prvious\\Filament\\Combobox\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@clear",
            "@prepare",
            "@php ./vendor/bin/testbench package:discover --ansi"
        ],
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint",
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ],
        "lint": [
            "@php vendor/bin/pint --parallel --ansi"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Prvious\\Filament\\Combobox\\ComboboxServiceProvider"
            ],
            "aliases": {
                "Combobox": "Prvious\\Filament\\Combobox\\Facades\\Combobox"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
